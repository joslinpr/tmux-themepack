# Start _themepack-options.tmuxsh
# Themepack format options
set -g @themepack-status-left-area-left-format "#S #W"
set -g @themepack-status-left-area-right-format "#I:#P"
set -g @themepack-status-right-area-left-format "%H:%M"
set -g @themepack-status-right-area-right-format "%d %b %Y"
set -g @themepack-window-status-current-format "#I:#W#F"
set -g @themepack-window-status-format "#I:#W#F"
# End  _themepack-options.tmuxsh
