# Themepack format options
set -gq @themepack-status-left-area-left-format "#H"          # Host
set -gq @themepack-status-left-area-right-format "#S"          # Session
set -gq @themepack-status-right-area-left-format "%H:%M"    # Hours : Minutes
set -gq @themepack-status-right-area-right-format "%d %b %Y"  # day Mon Year
set -gq @themepack-window-status-current-format "#I:#W#F"     # Index Window Flags
set -gq @themepack-window-status-format "#I:#W#F"             # Index Window Flags

